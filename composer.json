    "keywords": ["api", "rest", "backup", "json", "symfony bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mbeineris/BackupBundle.git"
        }
    ],
    "authors": [
        {
            "name": "Marius Beineris",
            "email": "mariusbeineris@gmail.com"
        }
    ],
    "require": {
